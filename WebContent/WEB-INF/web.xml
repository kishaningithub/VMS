    <description></description>
    <display-name>addVehicle</display-name>
    <servlet-name>addVehicle</servlet-name>
    <servlet-class>com.vms.servlets.addVehicle</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>addDriver</display-name>
    <servlet-name>addDriver</servlet-name>
    <servlet-class>com.vms.servlets.addDriver</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addVehicle</servlet-name>
    <url-pattern>/addVehicle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addDriver</servlet-name>
    <url-pattern>/addDriver</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Detail</display-name>
    <servlet-name>Detail</servlet-name>
    <servlet-class>com.vms.servlets.Detail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Detail</servlet-name>
    <url-pattern>/Detail</url-pattern>
  </servlet-mapping>
</web-app>